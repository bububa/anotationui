{
  "name": "anotationui",
  "version": "0.0.0",
  "author": {
    "name": "Syd Xu",
    "email": "prof.syd.xu@gmail.com",
    "url": "https://github.com/bububa/recgdb/anotationui"
  },
  "private": true,
  "scripts": {
    "bootstrap": "yarn install",
    "dev": "vite",
    "build": "vite build & echo ./build/script/postBuild.ts",
    "build:no-cache": "yarn clean:cache && npm run build",
    "serve": "npm run dev",
    "report": "cross-env REPORT=true npm run build",
    "preview": "vite preview",
    "build typecheck": "vuedx-typecheck . && vite build",
    "deploy": "gh-pages -d dist",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:lint-staged": "lint-staged -c ./.husky/lintstagedrc.js",
    "lint:pretty": "pretty-quick --staged",
    "test prod gzip": "http-server dist --cors --gzip -c-1"
  },
  "dependencies": {
    "@vicons/antd": "^0.11.0",
    "@vicons/ionicons5": "^0.11.0",
    "@vicons/material": "^0.11.0",
    "@vicons/tabler": "^0.11.0",
    "@vueuse/core": "^6.9.0",
    "axios": "^0.30.2",
    "naive-ui": "^2.20.3",
    "pinia": "^2.0.3",
    "qs": "^6.14.1",
    "vfonts": "^0.1.0",
    "vue": "^3.2.22",
    "vue-router": "^4.0.12",
    "vue-types": "^3.0.2",
    "vue@next": "^3.2.22",
    "vuedraggable": "^4.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vitejs/plugin-vue-jsx": "^1.2.0",
    "@vue/eslint-config-typescript": "^9.0.1",
    "async-validator": "^4.0.7",
    "autoprefixer": "^10.4.0",
    "dotenv": "^10.0.0",
    "enquirer": "^2.3.6",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-define-config": "^1.1.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "esno": "^0.12.0",
    "gh-pages": "^5.0.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "lint-staged": "^12.0.2",
    "postcss": "^8.3.11",
    "prettier": "^2.4.1",
    "pretty-quick": "^3.1.1",
    "rollup": "^2.79.2",
    "stylelint": "^14.1.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.0.0",
    "tailwindcss": "^2.2.19",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4",
    "unplugin-vue-components": "^0.17.2",
    "vite": "^5.4.21",
    "vite-plugin-compression": "^0.3.5",
    "vite-plugin-html": "^2.1.1",
    "vite-plugin-style-import": "^1.3.0",
    "vue-demi": "^0.12.1",
    "vue-eslint-parser": "^8.0.1",
    "webpack": "^5.94.0",
    "yarn-upgrade-all": "^0.5.4"
  },
  "lint-staged": {
    "*.{vue,js,ts,tsx}": "eslint --fix"
  },
  "keywords": [
    "vue",
    "naive-ui",
    "vue3",
    "ts",
    "tsx",
    "vision",
    "typescript"
  ]
}
